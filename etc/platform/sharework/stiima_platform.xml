<?xml version="1.0" encoding="UTF-8"?>

<ros>

	<command component="Robot" name="pickplace" >
	
		<dispatch-topic 
			name="/sharework/test/stiima/motion" 
			msg="sharework_task_planner_interfaces/MotionTaskExecutionRequestArray"
			publisher="it.cnr.istc.pst.sharework.task_planner.platform.rosbridge.stiima.MotionTaskRequestPublisher" />
			
		<feedback-topic 
			name="/sharework/test/stiima/motion/feedback" 
			msg="sharework_task_planner_interfaces/MotionTaskExecutionFeedback"
			delegate="it.cnr.istc.pst.sharework.task_planner.platform.rosbridge.stiima.MotionTaskFeedbackListener" />
			
	</command>
	
	<command component="Human" name="pickplace" >
	
		<dispatch-topic 
			name="/sharework/test/stiima/human" 
			msg="sharework_task_planner_interfaces/MotionTaskExecutionRequestArray"
			publisher="it.cnr.istc.pst.sharework.task_planner.platform.rosbridge.stiima.MotionTaskRequestPublisher" />
			
		<feedback-topic 
			name="/sharework/test/stiima/human/feedback" 
			msg="sharework_task_planner_interfaces/MotionTaskExecutionFeedback"
			delegate="it.cnr.istc.pst.sharework.task_planner.platform.rosbridge.stiima.MotionTaskFeedbackListener" />
			
	</command>
	
</ros>